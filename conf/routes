POST	/user						genapi.controller.UserController.createUser()
GET		/user						genapi.controller.UserController.getAllUser()
GET		/user/:id					genapi.controller.UserController.getUser(id: String)
PUT		/user/:id					genapi.controller.UserController.updateUser(id: String)
DELETE	/user/:id					genapi.controller.UserController.deleteUser(id: String)
POST	/user/:userId/post/:postId	genapi.controller.UserController.addPost(userId: String, postId: String)
DELETE	/user/:userId/post/:postId	genapi.controller.UserController.removePost(userId: String, postId: String)

POST	/post			genapi.controller.PostController.createPost()
GET		/post			genapi.controller.PostController.getAllPost()
GET		/post/:id		genapi.controller.PostController.getPost(id: String)
PUT		/post/:id		genapi.controller.PostController.updatePost(id: String)
DELETE	/post/:id		genapi.controller.PostController.deletePost(id: String)

GET     /assets/*file	controllers.Assets.versioned(path="/public", file: Asset)