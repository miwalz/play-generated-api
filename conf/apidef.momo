datatype String
datatype int
datatype boolean
datatype Date

morphiamodel Author {
	name: String
	born: java.util.Date 				// DATE
	married: boolean
	many articles: Article
	many embedded books: Book
	many aliases: String	
}

morphiamodel Book {
	title: String
	isbn: String
	pages: int
}

morphiamodel Article {
	title: String
	many authors: Author
}